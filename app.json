{
  "expo": {
    "name": "Taily",
    "slug": "Taily-app",
    "version": "1.0.0",
    "scheme": "taily",
    "newArchEnabled": false,
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saylekxd.Tailyapp",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["taily"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "taily",
            "CFBundleURLSchemes": ["taily"]
          }
        ]
      }
    },
    "android": {
      "package": "com.saylekxd.Tailyapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "taily.app"
            },
            {
              "scheme": "taily"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "taily://"
      },
      "eas": {
        "projectId": "d43f7904-5576-4aa4-afb4-25d881100a94"
      }
    }
  }
}
